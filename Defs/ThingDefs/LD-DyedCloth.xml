<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Randomly Colored Cloth should never actually
       exist in the game - it's a placedholder for
       the random-colored dying recipe.  As soon
       as it's created, it should change itself
       to a new cloth -->
  <ThingCategoryDef>
    <defName>LWM_NonExistant</defName>
    <label>For items that do not exist in the game</label>
    <resourceReadoutRoot>false</resourceReadoutRoot>
  </ThingCategoryDef>
  <ThingDef><!-- ParentName="LWM_ColoredClothBase">-->
    <defName>LWM_RandomColoredCloth</defName>
    <label>Randomly Colored Cloth</label>
    <description>Cloth in a random color</description>
    <thingClass>LWM.AreaRugs.RandomColoredCloth</thingClass>
    <thingCategories Inherit="False">
      <li>LWM_NonExistant</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Item/Resource/Cloth</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <!--TODO: Make it non-tradeable-->
    <!--Would be brilliant if I can make it not show up in filters :p -->
  </ThingDef>
  <ThingDef ParentName="LD-ResourceBase">
    <defName>RedCloth</defName>
    <label>red cloth</label>
    <description>Red Cloth made of dyed organic fibers.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Cloth</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <color>(125,50,60)</color>
    </graphicData>
     <statBases>
      <StuffPower_Armor_Sharp>0.36</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.18</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>18</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>18</StuffPower_Insulation_Heat>
      <MaxHitPoints>80</MaxHitPoints>
      <MarketValue>1.5</MarketValue>
      <Mass>0.026</Mass>
      <Flammability>1.2</Flammability>
      <DeteriorationRate>4</DeteriorationRate>
    </statBases>
    <stuffProps>
	  <statFactors>
        <Flammability>1.2</Flammability>
      </statFactors>
      <categories>
        <li>Fabric</li>
      </categories>
      <color>(125,50,60)</color>
      <allowColorGenerators>false</allowColorGenerators>
    </stuffProps>
    <thingCategories>
      <li>DyedCloth</li>
    </thingCategories>
  </ThingDef>
  
